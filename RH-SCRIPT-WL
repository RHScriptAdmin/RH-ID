if not game:IsLoaded() then
   game.Loaded:Wait()
   print("not loaded")
end
wait(2)
spawn(function()
wait(2)
if game.PlaceId == 735030788 then --divina park
    elseif game.PlaceId == 4728134592 then --Trading Hub
    else
        for _, v in pairs(workspace.DIAMONDS:GetDescendants()) do
            if v:IsA("TouchTransmitter") then
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
            end
        end
    end
    end)
spawn(function()
wait(2)
local SceptorTeleport = game:GetService("ReplicatedStorage").SceptorTeleport
if game.PlaceId == 1067560271 then
        SceptorTeleport:FireServer("New Royale")
    elseif game.PlaceId == 1765700510 then
        SceptorTeleport:FireServer("Enchantix")
end
end)
spawn(function()
while wait(0.01) do
for i,x in ipairs(game:GetService("Players").LocalPlayer.PlayerGui.CaptchaGui.Captcha.FloatArea:GetChildren()) do
if game:GetService("Players").LocalPlayer.PlayerGui.CaptchaGui.Enabled == true then
wait(1)
if x.Name == "FloatBox" then
firesignal(x.MouseButton1Click)
end
end
end
wait(3)
if game:GetService("Players").LocalPlayer.PlayerGui.CaptchaGui.Award.Visible == true then
firesignal(game:GetService("Players").LocalPlayer.PlayerGui.CaptchaGui.Award.Close.MouseButton1Down)
end
end
end)
