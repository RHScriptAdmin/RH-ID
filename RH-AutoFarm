--testing version 1.99
repeat wait() until game:IsLoaded() wait(0.5)
local EnchantixID = 1067560271
local SchoolID = 1765700510
local REP = game:GetService("ReplicatedStorage")

if game.PlaceId == EnchantixID or game.PlaceId == SchoolID then --Places Available
    spawn(function()--Diamonds Shit
        wait(2)
            for _, v in pairs(game:GetService("Workspace").DIAMONDS:GetDescendants()) do
                if v:IsA("TouchTransmitter") then
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0) --0 is touch
                    firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1) -- 1 is untouch
                end
            end
    end) --Diamonds Shit
    spawn(function()--Teleport System
        wait(5)
            if game.PlaceId == EnchantixID then --enchantix
                REP.SceptorTeleport:FireServer(REP.SceptorTeleport,"New Royale")
                while wait(15) do
                    REP.SceptorTeleport:FireServer(REP.SceptorTeleport,"New Royale")
                end
            elseif game.PlaceId == SchoolID then --school
                REP.SceptorTeleport:FireServer(REP.SceptorTeleport,"Enchantix")
                while wait(15) do
                    REP.SceptorTeleport:FireServer(REP.SceptorTeleport,"Enchantix")
                end
            end
    end)--Teleport System
end
